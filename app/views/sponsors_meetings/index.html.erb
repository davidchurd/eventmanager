<table>
  <tr>
    <th>Sponsor</th>
    <th>Meeting</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sponsor_meetings.each do |sponsor_meeting| %>
  <tr>
    <td><%= sponsor_meeting.sponsor_id %></td>
    <td><%= sponsor_meeting.meeting_id %></td>
    <td><%= link_to 'Show', sponsor_meeting %></td>
    <td><%= link_to 'Edit', edit_sponsor_meeting_path(sponsor_meeting) %></td>
